{
    "name": "Insightor",
    "description": "Youtube comment summarization",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "./popup.html",
        "default_icon": {
            "32": "/images/icon-32x32.png"
        }
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://*/*"
    ]
}